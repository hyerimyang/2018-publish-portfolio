/*//////////////////////Footer//////////////////*/
#footer { 
width:100%; 
height:450px;
background:url('../images/main/hd_visual.png') center center no-repeat; }
#footer_content { position:relative; width:100%; height:450px; z-index:99; background-color:rgba(0,0,0,0.8); }
#menu1 { width:150px; height:200px; position:absolute; top:30px; left:20px; text-align:center; } 
#menu2 { width:150px; height:200px; position:absolute; top:30px; right:20px; text-align:center; }
#menu1 li, #menu2 li{ margin-top:25px; font-size:11px; font-weight:600; color:#fff; cursor:pointer; }
#menu1 li:nth-child(1),#menu2 li:nth-child(1) { font-size:15px; }
#footer img { position:absolute; top:60px; left:0; right:0; bottom:0; margin:auto; }
#footer_txt { width:100%; text-align:center; color:#fff; position:absolute; bottom:40px; font-size:12px; }
#footer_txt p { margin-top:8px; font-weight:600;}