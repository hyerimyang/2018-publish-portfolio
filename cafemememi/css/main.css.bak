/*미디어쿼리*/
* { margin:0; padding:0; list-style:none; }

/*//////////////////////TOP//////////////////*/
/*	#main_wrap { width:400px; margin:auto; position:absolute;  top:0; }*/
#top_bg { 
	width:100%; 
	height:650px;
	background:url('../images/main/hd_visual.png') center center no-repeat;
	position:fixed; 
	top:0; 
	left:0; 
	z-index:-1;
}
#top_bg img { 
	width:350px;
	height:497px;
	position:absolute; 
	top:60px; 
	left:0;
	right:0; 
	margin:auto;
	position:fixed; 
	cursor:pointer;
}

/*//////////////////////Menu//////////////////*/
#menu { 
	width:100%; 
	height:100px; 
	background-color:#FFF; 
	margin-top:650px; 
	z-index:99; 
	position:absolute;
}
#menu img {
	width:142px; 
	height:60px; 
	margin:20px 0 20px 20px;
	cursor:pointer; 
}
#menu>ul { 
	width:25px; 
	height:25px; 
	float:right; 
	margin:40px 30px 0 0;
	position:relative;
	cursor:pointer; 
}
#menu>ul>li { 
	width:18px; 
	height:2px; 
	background-color:#000;
	border-radius:20px; 
	position:absolute; 
}
#menu>ul>li:nth-child(1) { 
	left:0; 
	top:0;
	transform-origin:0% 100%;
}
#menu>ul>li:nth-child(2) { 
	left:0; 
	top:7px; 
}
#menu>ul>li:nth-child(3) {
	left:0;
	top:14px;
	transform-origin:0% 100%; 
}
#menu_slide { 
	width:100%; 
	height:300px; 
	background-color:#FFF;
	clear:both; 
	position:absolute; 
	top:100px; 
	display:none; 
	z-index:99;
}
#menu_slide_list { 
	width:150px;
	height:300px;
	background-color:#fff;
}
#menu_slide_list li { 
	margin-bottom:10px; 
	font-size:15px;
	background-color:#fff;
	padding:10px; 
	border-bottom:1px solid gray; 
	padding-left:15px; 
	position:relative; 
	color:gray; 
	cursor:pointer;
}
#menu_slide_list li img { 
	width:19px; 
	height:19px;
	float:right; 
	position:absolute;
	top:-10px; 
	right:15px; 
}
#menu_slide_list li:hover { color:#ff33ff; }
img { vertical-align:top; }

/*//////////////////////Slide//////////////////*/
#slide_wrap { 
	width:100%;
	height:498px;
	position:relative; 
	overflow:hidden; 
	margin-top:750px;
}
#slide { 
	width:300%; 
	height:500px; 
	top:0; 
	left:0; 
	position:absolute; 
	cursor:pointer; 
}

#image1 { 
	background:url('../images/slide_img/image2.png') center top no-repeat; 
	float:left; 
	width:33.3333%; 
	height:500px;
}
#image2 { 
	background:url('../images/slide_img/image1.png') center top no-repeat; 
	float:left; 
	width:33.3333%; 
	height:500px; 
}
#image3 { 
	background:url('../images/slide_img/image3.png') center top no-repeat;
	float:left; 
	width:33.3333%; 
	height:500px;
}

#icon { 
	width:100%; 
	position:absolute;
	bottom:10px;
	text-align:center; 
}
#icon li { 
	width:8px; 
	height:8px; 
	border-radius:5px; 
	background-color:rgba(255,255,255,0.5);
	margin:3px; 
	display:inline-block; 
	cursor:pointer;
}
#icon li:nth-child(1) { background-color:#FFF; }

#cool_img { 
	background:url('../images/cool.png') center top no-repeat; 
	width:100%; 
	height:492px; 
	cursor:pointer; 
	position:relative; 
	background-color:red;
}
#cool_text { 
	width:400px; 
	height:180px;
	position:absolute; 
	left:20px; 
	top:150px;
	color:#fff; 
}
#cool_text p { font-size:42px; margin-bottom:20px; }
#cool_text h3 { font-size:15px; }
#cool_text h4 { font-size:13px; margin-bottom:20px; }
#cool_text h5 { font-size:13px; text-decoration:underline;}

/*//////////////////////Best Menu//////////////////*/
#best_wrap { 
	width:100%; 
	height:1500px; 
	background-color:#f7f1f0; 
	position:relative;
}

#top_text { 
	width:100%; 
	height:160px; 
	position:absolute;
	top:140px; 
	text-align:center;
} 
#top_text p { font-size:25px; }
#top_text div { 
	width:35px; 
	height:20px; 
	border-bottom:2px solid #000;
	margin:auto; 
}
#top_text h4 {
	color:gray;
	font-size:14px;
	font-weight:600; 
margin-top:20px;
}

#bottom_img { 
	width:100%; 
	height:1100px; 
	position:relative; 
	top:300px;
	cursor:pointer;
}
#image ul { width:100%; height:210px;  }
#image li { 
	width:50%;
	height:500px; 
	float:left;
	position:relative;
	z-index:9;
}
#image li img { 
	width:90%;
	position:absolute;
	margin:auto; 
	left:0;
	right:0;
	top:0;
	bottom:0;
}

#bottom_text {
	width:100%; 
	height:1000px; 
	position:absolute; 
	top:450px; 
}
#bottom_text ul { width:100%; height:500px; }
#bottom_text li { 
	text-align:center;
	float:left; 
	width:50%; 
	height:500px; 
}
#bottom_text li p { font-size:15px; margin-bottom:10px; }
#bottom_text li h5 { font-size:11px; color:gray; }

/*//////////////////////Shop//////////////////*/
#shop_wrap { 
	width:100%; 
	background-color:#f9f9f9; 
	position:relative;
}
#shop_top_text { 
	width:100%; 
	height:200px;
	text-align:center;
	padding-top:130px; 
}
#shop_top_text p { font-size:25px; }
#shop_top_text div { 
	width:35px;
	height:20px;
	border-bottom:2px solid #000; 
	margin:auto; 
}
#shop_top_text h4 {
	color:gray;
	font-size:14px;
	font-weight:600; 
	margin-top:20px; 
}


#shop_image {
	background-color:#f9f9f9; 
	width:100%; 
	position:relative; 
}
#shop_image ul { width:100%; text-align:center; }
#shop_image li { 
	width:96%; 
	margin:auto; 
	margin-bottom:20px; 
	cursor:pointer; 
	background-color:#000; 
	position:relative;
}
#shop_image li:nth-child(1) img { width:100%; vertical-align:top; }
#shop_image li:nth-child(1) div { 
	position:absolute;
	top:240px;
	left:0; 
	color:#fff; 
	width:100%; 
	text-align:center;
	opacity:0; 
	z-index:99; 
}
#shop_image li:nth-child(1):hover img { opacity:0.5; transition:1s; }
#shop_image li:nth-child(1):hover div { opacity:1; transition:1s; }
#shop_image li:nth-child(2) img { width:100%; vertical-align:top; }
#shop_image li:nth-child(3) img { width:100%; vertical-align:top; }
#shop_image li:nth-child(3) div { 
	position:absolute;
	top:90px; 
	left:0; 
	color:#fff;
	width:100%; 
	text-align:center;
	opacity:0; 
	z-index:99; 
}
#shop_image li:nth-child(3):hover img { opacity:0.5; transition:1s; }
#shop_image li:nth-child(3):hover div { opacity:1; transition:1s; }
#shop_image li:nth-child(4) img { width:100%; vertical-align:top; }
#shop_image li:nth-child(4) div { 
	position:absolute; 
	top:240px; 
	left:0;
	color:#fff;
	width:100%;
	text-align:center;
	opacity:0; 
	z-index:99; 
}
#shop_image li:nth-child(4):hover img { opacity:0.5; transition:1s; }
#shop_image li:nth-child(4):hover div { opacity:1; transition:1s;}
#shop_image li:nth-child(5) img { width:100%; vertical-align:top; }
#shop_image li:nth-child(5) div { 
	position:absolute;
	top:240px;
	left:0; 
	color:#fff; 
	width:100%; 
	text-align:center; 
	opacity:0; 
	z-index:99; 
}
#shop_image li:nth-child(5):hover img { opacity:0.5; transition:1s; }
#shop_image li:nth-child(5):hover div { opacity:1; transition:1s; }
#shop_image li:nth-child(6) img { width:100%; vertical-align:top; }
#shop_image li div h5 { text-decoration:underline; }

#shop_slide_wrap { 
	width:100%; 
	height:180px; 
	position:relative; 
	margin-top:100px; 
	overflow:hidden; 
	padding-bottom:80px;
} 
#shop_slide {
	width:2408px; 
	height:180px; 
	position:absolute; 
	left:-1000px; 
}
#shop_slide ul li { display:inline-block; }

/*//////////////////////Caffe//////////////////*/
#caffe_top_text_wrap { 
	width:100%; 
	height:370px;
	background-color:#fff; 
	position:relative;
}
#caffe_top_text { 
	width:100%; 
	text-align:center; 
	position:absolute;
	top:130px; 
}
#caffe_top_text p { font-size:25px; }
#caffe_top_text h3 { font-weight:100; font-size:18px; }
#caffe_top_text div { 
	width:35px; 
	height:20px;
	border-bottom:2px solid #000;
	margin:auto; 
}
#caffe_top_text h4 { 
	color:gray;
	margin-top:20px;
	font-size:14px;
	font-weight:600;
}
#attach { 
	width:100%; 
	height:520px; 
	background: url('../images/store_bg.jpg') center center no-repeat; 
	background-attachment:fixed; 
}
#caffe_img_wrap { width:100%; }
#caffe_img { 
	width:100%; 
	padding-top:80px; 
	background-color:#FFF; 
	padding-bottom:80px; 
}
#caffe_img ul { width:100%; }
#caffe_img li { 
	width:95%; 
	margin:auto; 
	height:350px; 
	background-color:#FFF; 
	cursor:pointer;
}
#caffe_img li img { width:100%; }
#caffe_img li p { 
	font-size:14px;
	font-weight:600;
	text-align:center; 
	margin-top:10px; 
}
#caffe_img li h6 { 
	color:gray;
	text-align:center; 
	margin-top:12px;
}

/*//////////////////////Trend//////////////////*/
#trend_wrap {
	width:100%; 
	background-color:#fff6fc;
	padding:1%; 
	padding-bottom:150px;
}
#trend_top_text { 
	width:100%;
	padding-top:130px;
	text-align:center;
}
#trend_top_text p { font-size:25px; }
#trend_top_text div { 
	width:35px; 
	height:20px; 
	border-bottom:2px solid #000; 
	margin:auto;
}
#trend_top_text h4 { 
	color:#acacac; 
	margin-top:20px;
	font-size:14px; 
	font-weight:600; 
} 
#trend_img {
	width:98%; 
	margin-top:70px;
	overflow:hidden;
}
#trend_img img { padding:1%; cursor:pointer; }
#trend_img img:nth-child(1),#trend_img img:nth-child(2),#trend_img img:nth-child(4) { 
	float:left;
	width:47%;
} 
#trend_img img:nth-child(3) { clear:both; float:left; width:47%; }
#trend_img img:nth-child(5) { clear:both; padding-bottom:6%; width:96%; }
#trend_img img:nth-child(6) { width:96%; } 
#trend_img img:nth-child(7),#trend_img img:nth-child(8),#trend_img img:nth-child(10) { 
	float:left;
	width:47%; 
}
#trend_img img:nth-child(9) {
	clear:both; 
	float:left; 
	width:47%;
}

/*//////////////////////Insta//////////////////*/
